<script>
		var MainCtrl = function ($scope, $location) {
			$scope.mainNavigationTemplateUrl = '/App/Components/main-navigation/main-navigation.html';
			$scope.sidebarTemplateUrl        = '/App/Components/sidebar/sidebar.html';
			$scope.footerTemplateUrl         = '/App/Components/footer/footer.html';
			$scope.locationPath              = $location.url();
		};

		function reCapthcha() {
			var ddo = {
				restrict: 'AE',
				scope: {},
				require: 'ngModel',
				link: link
			};
			return ddo;

			function link(scope, elm, attrs, ngModel) {
				var id;
				ngModel.$validators.captcha = function (modelValue, ViewValue) {
					// if the viewvalue is empty, there is no response yet,
					// so we need to raise an error.
					return !!ViewValue;
				};

				function update(response) {
					ngModel.$setViewValue(response);
					ngModel.$render();
				}

				function expired() {
					grecaptcha.reset(id);
					ngModel.$setViewValue('');
					ngModel.$render();
					// do an apply to make sure the  empty response is
					// proaganded into your models/view.
					// not really needed in most cases tough! so commented by default
					// scope.$apply();
				}

				function iscaptchaReady() {
					if (typeof grecaptcha !== 'object') {
						// api not yet ready, retry in a while
						return setTimeout(iscaptchaReady, 250);
					}
					id = grecaptcha.render(
						elm[0], {
							// put your own sitekey in here, otherwise it will not
							// function.
							'sitekey': '6Lc1qCIUAAAAAN-mQ-wt3-_Q49kOvt7V17zSS0lz',
							callback: update,
							'expired-callback': expired
						}
					);
				}

				iscaptchaReady();
			}
		}

		var app = angular.module('web.main', [
			// Angular Modules
			'ngRoute',
			'ngCookies',
			'ngSanitize',
			'ui.bootstrap',
			'angular.filter',
			// Components
			'tools.mainNavigation',
			'tools.sidebar',
			'tools.footer',
			// Pages
			'tools.homepage',
			'tools.lol-time-played',
			'tools.lol-champions-k',
			'tools.champions',
			'tools.term-dictionary',
			'tools.ping'
		])
		.controller('MainCtrl', ['$scope', '$location', MainCtrl])
		.directive('reCaptcha', reCapthcha)
		.config(['$routeProvider', '$locationProvider', '$httpProvider', function ($routeProvider, $locationProvider, $httpProvider) {
			$locationProvider.hashPrefix('!');
			$locationProvider.html5Mode(true);
			// routing
			$routeProvider
			.when('/', {
				templateUrl: '/App/Pages/index/index.html',
				controller: 'HomepageCtrl'
			})
			.when('/kac-saat-lol-oynadin', {
				templateUrl: '/App/Pages/lol-time-played/lol-time-played.html',
				controller: 'LoLTimePlayedCtrl'
			})
			.when('/seninki-kac-k', {
				templateUrl: '/App/Pages/lol-champions-k/lol-champions-k.html',
				controller: 'LoLChampionsKCtrl'
			})
			.when('/sampiyonlar', {
				templateUrl: '/App/Pages/champions/champions.html',
				controller: 'ChampionsCtrl'
			})
			.when('/aov-sampiyonlar', {
				templateUrl: '/App/Pages/aov-champions/aov-champions.html',
				controller: 'AovChampionsCtrl'
			})
			.when('/lol-terimler-sozlugu', {
				templateUrl: '/App/Pages/term-dictionary/term-dictionary.html',
				controller: 'TermDictionaryCtrl'
			})
			.when('/ping', {
				templateUrl: '/App/Pages/ping/ping.html',
				controller: 'PingCtrl'
			})
			.when('/mavi-oz-hesapla', {
				templateUrl: '/App/Pages/mavi/mavi.html',
				controller: 'MaviController'
			})
			.when('/sampiyon-rehberi', {
				templateUrl: '/App/Pages/sampiyon-rehberi/sampiyon-rehberi.html'
			})
			.otherwise({
				redirectTo: '/'
			});
		}])
		.filter('range', function() {
			return function(input, total) {
				total = parseInt(total);

				for (var i=0; i<total; i++) {
					input.push(i);
				}

				return input;
			};
		})
		.directive('bsTooltip', function(){
			return {
				link: function(scope, element, attrs){
					$(element).hover(function(){
						$(element).tooltip('show');
					}, function(){
						$(element).tooltip('hide');
					});
				}
			};
		})
		/*.config(function ($httpProvider) {
			$httpProvider.defaults.headers.common  = {
			};
			$httpProvider.defaults.headers.options = {
				'Access-Control-Allow-Origin': '*'
			};
			$httpProvider.defaults.headers.put     = {};
			$httpProvider.defaults.headers.patch   = {};
		})*/;

</script>
