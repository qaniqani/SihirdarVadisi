<script>
    // Variables
    var corsURL = "";
    var playerID = "";
    var xhr = "";

    // Get Parameters by Name
    function getParameterByName(name, url) {
        if (!url) {
            url = window.location.href;
        }
        name = name.replace(/[\[\]]/g, "\\$&");
        var regex = new RegExp("[?&]" + name + "(=([^&#]*)|&|#|$)"),
            results = regex.exec(url);
        if (!results) return null;
        if (!results[2]) return '';
        return decodeURIComponent(results[2].replace(/\+/g, " "));
    }

    // Query String Control
    var server = getParameterByName('server');
    var playername = getParameterByName('player-name')
    if (server != null && playername != null) {
        var corsURL = "https://" + server.toLowerCase() + ".api.riotgames.com/api/lol/" + server + "/v1.4/summoner/by-name/" + playername + "?api_key=RGAPI-AC33441F-8A24-4CB9-8AD1-476C2974ED4A";
        load(corsURL);
    } else {
        // Do sex.
    }

    function load(corsURL, callback) {
        var xhr = new XMLHttpRequest();

        xhr.onreadystatechange = function () {
            if (xhr.readyState === 4) {
                console.log(xhr); //Outputs a DOMString by default
            }
        }

        xhr.open('GET', corsURL, true);
        xhr.send('');
    }


</script>

